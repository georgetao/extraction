CNGS_MGT_FLG "SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF","SELF";
PRIOR_TYPE "OTHER";
LOSS_CMP_FLG "NO";
WSCC_TAG "";
